{
  "compilerOptions": {
    "target": "ES6",
    "module": "CommonJS",
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "paths": {
      "~_assets/*": ["./src/assets/*"],
      "~_components/*": ["./src/Components/*"],
      "~_lazy/*": ["./src/Components/Lazy*"],
      "~_pages/*": ["./src/Components/Pages*"],
      "~_landing/*": ["./src/Components/Pages/Landing*"],
      "~_partials/*": ["./src/Components/Partials*"],
      "~_animations/*": ["./src/Components/Partials/Animation*"],
      "~_cards/*": ["./src/Components/Partials/cards*"],
      "~_particles/*": ["./src/Components/Particles*"],
      "~_screens/*": ["./src/Components/Screens*"],
      "~_navigations/*": ["./src/Navigation/*"],
      "~_providers/*": ["./src/Providers/*"],
      "~_routes/*": ["./src/Routes/*"],
      "~_stores/*": ["src/assets/Store/*"],
      "~_templates/*": ["./src/Template*"],
      "~_utils/*": ["./src/utils*"],
      "~_src/*": [
        "src/*"
    ]
  }
  },
  "include": ["src"],
  "exclude":["node_modules","build"]
}